<script>
$(document).ready(function() {
	//-----------------------------------------------------------------------
	// Description :: summernote 설정
	//----------------------------------------------------------------------	
	$("#summernote").summernote({
		lang: 'ko-KR',
		height:300,
		focus:true
	});
	
	//-----------------------------------------------------------------------
	// Description :: summernote 에 content 반영하기
	//----------------------------------------------------------------------
	//var content='테스트';
	//$("#summernote").code(content);

	//-----------------------------------------------------------------------
	// Description :: summernote 에서 content 가져오기
	//----------------------------------------------------------------------	
	$('#btnCreate').click(function(e) {
		var content = $("#ta-editor").data("kendoEditor").value();
		var content = $("#summernote").code();
		$('#ta-content').val('');
		$('#ta-content').val(content);
	});
	
	$('#btnTest').click(function(e) {
		alert( mint.tooltip.isExistTooltip('AN-01-00-001') );
	});		

});
